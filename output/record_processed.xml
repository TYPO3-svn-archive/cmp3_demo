        <field name="title" type="text" format="line">
          <meta/>
          <value>NASA is Working on a Real-Life Star Trek Warp Drive</value>
        </field>
        <field name="datetime" type="datetime" format="datetime">
          <meta/>
          <value>2012-09-21T10:22:00+02:00</value>
        </field>
